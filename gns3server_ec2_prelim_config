#!
apt update
apt install qemu qemu-kvm libvirt-daemon-system libvirt-clients bridge-utils virt-manager iouyap iolib libffi-dev
adduser ubuntu libvirt
adduser ubuntu kvm
add-apt-repository ppa:gns3/ppa
apt update
apt install gns3-gui gns3-server
wget https://github.com/GNS3/iouyap/archive/master.zip
unzip master.zip
cd iouyap-master
make
make install
systemctl restart libvirtd
service gns3server restart
gns3server


